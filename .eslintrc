{
  "extends": "eslint:recommended",
  "rules": {
    "indent": [2, 2],
    "comma-style": [2, "last"],
    "consistent-this": [2, "_this"],
    "no-nested-ternary": 2,
    "quotes": [2, "single"],
    "semi": 2
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true,
    "modules": true,
    "experimentalObjectRestSpread": true
  },
  "plugins": [
    "react"
  ]
}
